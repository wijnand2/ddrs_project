(VAR x y)
(RULES
neg(0) -> 0
neg(neg(x)) -> x

S(neg(S(x))) -> neg(x)

plus(x, 0) -> x
plus(0, x) -> x
plus(x, neg(S(0))) -> neg(S(neg(x)))
plus(neg(S(0)), x) -> neg(S(neg(x)))
plus(S(x), S(y)) -> S(S(plus(x, y)))
plus(S(x), neg(S(y))) -> plus(x, neg(y))
plus(neg(S(x)), S(y)) -> plus(neg(x), y)
plus(neg(S(x)), neg(S(y))) -> neg(S(S(plus(x, y))))

mult(x, 0) -> 0
mult(x, neg(S(0))) -> neg(x)
mult(x, S(y)) -> plus(mult(x, y), x)
mult(x, neg(S(y))) -> neg(mult(x, S(y)))
)
