(VAR x y z w)
(RULES
N(0) -> 0
N(neg(x)) -> neg(N(x))

neg(0) -> 0
neg(neg(x)) -> x

S(S(x)) -> plus(x, N(S(0)))
S(neg(x)) -> neg(P(x))
S(P(x)) -> x

P(0) -> neg(S(0))
P(S(x)) -> x
P(N(x)) -> S(N(P(x)))
P(neg(x)) -> neg(S(x))

plus(x, 0) -> x
plus(x, S(y)) -> plus(S(x), y)
plus(x, P(y)) -> plus(P(x), y)
plus(x, N(y)) -> plus(plus(x, y), y)
plus(x, neg(y)) -> subt(x, y)
plus(subt(x, y), y) -> x

subt(x, 0) -> x
subt(x, S(y)) -> subt(P(x), y)
